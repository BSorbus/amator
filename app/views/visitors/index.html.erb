
<div class="row">
  <div class="col-md-12">

		<div class="page-header">
		  <h1><%= t('pages.visitors.index.title') %> <small><%= t('pages.visitors.index.title_sub') %></small></h1>
		</div>

	</div>
</div>


<p>remote_ip: <%= request.remote_ip %></p>
<p>request.host: <%= request.host %></p>
<p>request.domain: <%= request.domain(2) %></p>
<p>request.request_parameters: <%= request.request_parameters %></p>
<p>request.query_parameters: <%= request.query_parameters %></p>
<p>request.path_parameters: <%= request.path_parameters %></p>

<div class="row">
  <div class="col-md-3">
    <div class="thumbnail">
      <div class="caption">
        <h4><%= t('pages.visitors.index.sub_individuals') %></h4>
        <p><%= t('pages.visitors.index.sub_individuals_text') %></p>

        <%= link_to individuals_path do %>
          <span class="btn btn-primary glyphicon glyphicon-user"> <%=t "link_to.individuals" %></span>
        <% end %>

      </div>
    </div>
  </div>

  <div class="col-md-3">
    <div class="thumbnail">
      <div class="caption">
        <h4><%= t('pages.visitors.index.sub_clubs') %></h4>
        <p><%= t('pages.visitors.index.sub_clubs_text') %></p>

        <%= link_to clubs_path do %>
          <span class="btn btn-primary glyphicon glyphicon-home"> <%=t "link_to.clubs" %></span>
        <% end %>

      </div>
    </div>
  </div>

  <div class="col-md-3">
    <div class="thumbnail">
      <div class="caption">
        <h4><%= t('pages.visitors.index.sub_mfree_individuals') %></h4>
        <p><%= t('pages.visitors.index.sub_mfree_individuals_text') %></p>
        <p><a href="#" class="btn btn-primary" role="button">Button</a> </p>
      </div>
    </div>
  </div>

  <div class="col-md-3">
    <div class="thumbnail">
      <div class="caption">
        <h4><%= t('pages.visitors.index.sub_mfree_clubs') %></h4>
        <p><%= t('pages.visitors.index.sub_mfree_clubs_text') %></p>
        <p><a href="#" class="btn btn-primary" role="button">Button</a> </p>
      </div>
    </div>
  </div>


</div>